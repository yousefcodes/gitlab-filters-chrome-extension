{
  "manifest_version": 2,

  "name": "YousefCodes GitLab Plugin",
  "description": "This extension allows you to easily filter by name",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],

  "web_accessible_resources": [
    "config.json"
  ]
}